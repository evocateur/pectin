{
  "command": {
    "publish": {
      "allowBranch": "latest",
      "conventionalCommits": true,
      "message": "chore(release): Publish packages"
    }
  },
  "ignoreChanges": [
    "**/__tests__/**",
    "**/test/**",
    "**/*.test.js",
    "**/*.md"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
